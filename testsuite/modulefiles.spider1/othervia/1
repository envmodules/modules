#%Module
if {[info exists env(TESTSUITE_SPIDER_OTHERMODPATH1)]} {
    switch -- $env(TESTSUITE_SPIDER_OTHERMODPATH1) {
        use_modpath2 {
            module use $env(TESTSUITEDIR)/modulefiles.spider2
        }
        prepend_modpath_inref3 {
            if {[module-info mode load]} {
                setenv TESTSUITE_SPIDER_INREF er2
            }
            prepend-path MODULEPATH $env(TESTSUITEDIR)/modulefiles.spid\$TESTSUITE_SPIDER_INREF
        }
        use_modpath2_and_conflict_moda {
            conflict moda
            module use $env(TESTSUITEDIR)/modulefiles.spider2
        }
        conflict_moda {
            conflict moda
        }
        use_other2_and_conflict_moda {
            conflict moda
            module use $env(TESTSUITEDIR)/modulefiles.other2
        }
        use_2_and_conflict_moda {
            conflict moda
            module use $env(TESTSUITEDIR)/modulefiles.2
        }
        use_refvar_and_conflict_moda {
            conflict moda
            module use $env(TESTSUITEDIR)/modulefiles.oth\$TESTSUITE_SPIDER_INREF
        }
    }
}
